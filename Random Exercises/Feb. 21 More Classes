import numpy as np
def compute(x):
    return np.nanmean(x)
class Create(object):
    def __init__(self,name, data):
        self.name = name
        self.data = data
    def func(self):
        return compute(np.arange(100)) * self.data

x = np.arange(100)
thing = Create('Example', x)
print(thing.func())
